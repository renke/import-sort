{
  "name": "vscode-import-sort",
  "displayName": "vscode-import-sort",
  "description": "The Visual Studio Code Implementation of ImportSort",
  "version": "0.0.1",
  "publisher": "koelpinl",
  "engines": {
    "vscode": "^1.7.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.importSort",
    "onLanguage:typescript",
    "onLanguage:javascript"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [{
      "command": "extension.importSort",
      "title": "Import Sort"
    }]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "import-sort": "^2.4.0",
    "import-sort-config": "^2.4.0",
    "import-sort-parser-typescript": "^2.2.1",
    "import-sort-style-eslint": "^2.4.0",
    "import-sort-style-renke": "^2.4.0",
    "loophole": "^1.1.0"
  }
}
