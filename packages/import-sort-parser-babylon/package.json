{
  "name": "import-sort-parser-babylon",
  "version": "4.2.0",
  "description":
    "An import-sort parser based on the JavaScript parser Babylon.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc -p src",
    "build:watch": "tsc -p src -w",
    "test": "mocha --require ts-node/register --recursive \"test/**/*.ts\"",
    "test:watch":
      "mocha -w --require ts-node/register --recursive \"test/**/*.ts\""
  },
  "author": "Renke Grunwald <renke@renke.org> (https://github.com/renke)",
  "repository": "renke/import-sort",
  "license": "ISC",
  "devDependencies": {
    "@types/babel-traverse": "^6.7.17",
    "@types/babel-types": "^6.7.16",
    "@types/babylon": "^6.16.1",
    "@types/chai": "^4.0.0",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8",
    "chai": "^4.0.2",
    "import-sort-parser": "^4.2.0",
    "mocha": "^4",
    "ts-node": "^3.0.6",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4"
  },
  "dependencies": {
    "@babel/traverse": "^7.0.0-beta.38",
    "@babel/types": "^7.0.0-beta.38",
    "babylon": "^7.0.0-beta.38",
    "find-line-column": "^0.5.2"
  }
}
